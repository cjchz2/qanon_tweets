#Create User Dimension table:
CREATE TABLE `user` (
  `USER_NO` mediumint(9) NOT NULL AUTO_INCREMENT,
  `HANDLE` varchar(255) NOT NULL,
  `NAME` varchar(255) NOT NULL,
  `BIO` varchar(255) DEFAULT NULL,
  `FOLLOWER_COUNT` int(10) unsigned DEFAULT NULL,
  `LOCATION` varchar(255) DEFAULT NULL,
  `VERIFIED_FLAG` char(1) DEFAULT NULL,
  PRIMARY KEY (`USER_NO`)
);

#Create staging table
CREATE TABLE TWITTER.QANON_TWEETS_STG (
ID BIGINT NOT NULL ,
USER_NAME VARCHAR(255),
USER_NO MEDIUMINT,
DATE DATE NOT NULL,
TEXT TEXT NOT NULL ,
FAVORITED CHAR(1) ,
RETWEET_COUNT BIGINT
);
#Create fact table
CREATE TABLE TWITTER.QANON_TWEETS_FACT (
TWEET_NO MEDIUMINT NOT NULL AUTO_INCREMENT,
ID BIGINT NOT NULL ,
USER_NO MEDIUMINT NOT NULL,
DATE DATE NOT NULL,
TEXT TEXT NOT NULL ,
FAVORITED CHAR(1) ,
RETWEET_COUNT BIGINT,
CONSTRAINT PRIMARY KEY (TWEET_NO),
CONSTRAINT FOREIGN KEY (USER_NO) REFERENCES USER(USER_NO)
);
